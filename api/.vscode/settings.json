{
    "eslint.validate": [
      "javascript",
      "javascriptreact",
      "typescript",
      "typescriptreact",
    ],
    "[javascript]": {
      "editor.formatOnSave": false,
      "editor.codeActionsOnSave": [
        "source.fixAll",
      ]
    },
    "[javascriptreact]": {
      "editor.formatOnSave": false,
      "editor.codeActionsOnSave": [
        "source.fixAll",
      ]
    },
    "[typescript]": {
      "editor.formatOnSave": false,
      "editor.codeActionsOnSave": [
        "source.fixAll",
      ]
    },
    "[typescriptreact]": {
      "editor.formatOnSave": false,
      "editor.codeActionsOnSave": [
        "source.fixAll",
      ]
    },
    "npm.packageManager": "yarn",
    "typescript.tsdk": "./node_modules/typescript/lib",
    "typescript.preferences.importModuleSpecifier": "project-relative"
  }
  