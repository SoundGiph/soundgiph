{
  "name": "nest-typescript-starter",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "env-cmd -f .env.dev nodemon --config nodemon.json",
    "start:debug": "nest start --debug --watch",
    "start:prod": "env-cmd -f .env node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "clean:dists": "rm -rf ./dist",
    "clean-install": "rm -rf ./dist && rm -rf ./node_modules && yarn",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand --detectOpenHandles",
    "start:dev:db": "./start-db.sh",
    "typeorm:dev": "env-cmd -f .env.dev ts-node ./node_modules/typeorm/cli.js --config ./src/config/orm/ormconfig.ts",
    "migration:dev": "yarn typeorm:dev migration:run",
    "migration:create:dev": "yarn typeorm:dev migration:create",
    "migration:generate:dev": "yarn typeorm:dev migration:generate",
    "schema-log:dev": "yarn typeorm:dev schema:log",
    "typeorm:prod": " ts-node ./node_modules/typeorm/cli.js --config ./src/config/orm/ormconfig-prod.ts",
    "migration:prod": "yarn typeorm:prod migration:run",
    "migration:create:prod": "yarn typeorm:prod migration:create",
    "migration:generate:prod": "yarn typeorm:prod migration:generate",
    "schema-log:prod": "yarn typeorm:prod schema:log"
  },
  "dependencies": {
    "@azure/storage-blob": "^12.8.0",
    "@nestjs/common": "^8.1.1",
    "@nestjs/config": "^1.1.5",
    "@nestjs/core": "^8.1.1",
    "@nestjs/cqrs": "^8.0.0",
    "@nestjs/platform-express": "^8.1.1",
    "@nestjs/typeorm": "^8.0.2",
    "babel-jest": "^27.4.4",
    "dotenv": "^10.0.0",
    "faker": "^5.5.3",
    "node-fetch": "^3.1.0",
    "nodemon": "^2.0.15",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0",
    "typeorm": "^0.2.41",
    "env-cmd": "latest" 
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.3",
    "@nestjs/schematics": "^8.0.4",
    "@nestjs/testing": "^8.1.1",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.9",
    "@types/jest": "^27.0.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "jest": "^27.3.0",
    "prettier": "^2.4.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.3.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
